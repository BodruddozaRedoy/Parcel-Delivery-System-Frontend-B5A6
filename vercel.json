{
  "version": 2,
  "name": "parcel-delivery-system-frontend-b5a6",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "buildCommand": "npm run build",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://parcel-management-system-b5-a5.vercel.app/api/:path*" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
